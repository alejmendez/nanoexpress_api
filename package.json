{
  "name": "nano_express",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest --detectOpenHandles --silent --config ./src/test/jest.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./src/test/jest-e2e.json",
    "test:e2e:watch": "jest --config ./src/test/jest-e2e.json --watch",
    "migrations:generate": "npm run typeorm -- migration:generate -n",
    "migrations:create": "npm run typeorm -- migration:create -n",
    "migrations:run": "npm run typeorm -- migration:run",
    "migrations:show": "npm run typeorm -- migration:show",
    "migrations:drop": "npm run typeorm -- migration:drop",
    "generate": "ts-node -r tsconfig-paths/register src/core/generator/GeneratorCommand.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.0",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "tsconfig-paths-jest": "^0.0.1"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/commander": "^2.12.2",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.1",
    "@types/handlebars": "^4.1.0",
    "@types/inquirer": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.180",
    "@types/node-polyglot": "^2.4.2",
    "@types/prompt": "^1.1.2",
    "@types/validator": "^13.7.2",
    "@types/winston": "^2.4.4",
    "bcrypt": "^5.0.1",
    "commander": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "nanoexpress": "5.2.2",
    "node-polyglot": "^2.4.2",
    "pg": "^8.7.3",
    "prompt": "^1.2.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typeorm": "^0.2.45",
    "typescript": "^4.6.2",
    "validator": "^13.7.0",
    "winston": "^3.6.0"
  }
}
