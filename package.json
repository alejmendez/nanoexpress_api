{
  "name": "nano_express",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:generate": "npm run typeorm -- migration:generate -n",
    "migrations:create": "npm run typeorm -- migration:create -n",
    "migrations:run": "npm run typeorm -- migration:run",
    "migrations:show": "npm run typeorm -- migration:show",
    "migrations:drop": "npm run typeorm -- migration:drop",
    "generate": "ts-node src/core/generator/GeneratorCommand.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/ajv": "^1.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.1",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.180",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.0",
    "eslint": "^8.11.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/handlebars": "^4.1.0",
    "@types/inquirer": "^8.2.0",
    "@types/prompt": "^1.1.2",
    "ajv": "^8.10.0",
    "bcrypt": "^5.0.1",
    "commander": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "handlebars": "^4.7.7",
    "inquirer": "^8.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "nanoexpress": "5.2.2",
